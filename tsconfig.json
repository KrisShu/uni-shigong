{
    "extends": "@vue/tsconfig/tsconfig.json",
    "compilerOptions": {
        "target": "ESNext",
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"]
        },
        "lib": ["esnext", "dom"],
        "types": ["@dcloudio/types", "pinia"]
    },
    "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.vue"]
}
