import { http } from '@/utils/request';

// 登录
export function loginAPI(data: any) {
    return http.post('/login', data);
}
// 刷新登录
export function refreshTokenAPI(data: any) {
    return http.post('/emp/common/refreshToken', data);
}

// 员工端上传文件

export function EMP_fileUploadAPI(data: any) {
    return http.post('/emp/common/fileUpload', data);
}

//施工设备
export function EMP_getDevice() {
    return http.post('/emp/data/getDevice');
}
// 官网
export function websiteAPI() {
    return http.post('/login/website');
}
export default {
    loginAPI,
    EMP_fileUploadAPI,
    EMP_getDevice,
    refreshTokenAPI,
    websiteAPI,
};
